//	cout<<"I= "<<i<<"";
//}
#include <iostream>
using namespace std;
int swap (int &a, int b)
{
    int t;
    t=a;
    a=b;
    b=t;
}
int main ()
{
    int i,j;
    cout<<"Enter two the number " ;
    cin>>i>>j;
    swap(i,j);
    cout<<"After swapping the i "<<i<<" and j "<<j<<"  " ;
    return 0;
}

